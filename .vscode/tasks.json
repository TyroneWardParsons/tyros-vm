{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "TVM C build task.",
			"command": "C:\\MinGW\\bin\\gcc.exe",
			"args": [
				".\\tvm.c",
				".\\main.c",
				"-o",
				".\\bin\\tvm.exe"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build"
		}
	]
}